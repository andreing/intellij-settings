<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
    <option name="USE_CUSTOM_SOFT_WRAP_INDENT" value="true" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="QUICK_DOC_ON_MOUSE_OVER_DELAY_MS" value="750" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="SHOW_PARAMETER_NAME_HINTS" value="false" />
  </component>
  <component name="LiveEditOptions" autoDelay="2000" restartIfHotSwapFailed="true" />
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\bnote\b.*">
      <option name="FOREGROUND" value="20a5c0" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="ab7415" />
      <option name="EFFECT_TYPE" value="5" />
    </pattern>
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssColorPreviewInGutter" value="false" />
    <option name="showCssInlineColorPreview" value="true" />
    <option name="tagTreeHighlightingLevelCount" value="8" />
    <option name="tagTreeHighlightingOpacity" value="20" />
  </component>
</application>